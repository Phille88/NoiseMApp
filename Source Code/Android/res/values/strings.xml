    <!-- About -->
    <string name="aboutTitle">About</string>
    <string name="aboutExplanation">Hi, I\'m Philippe De Croock and I have developed this app for my master thesis: \"Generating Sound Level Maps through Gamification\".\n\nBy using this app, you help me with providing test results!\n\nIf you have any questions, please email me at philippe.decroock@student.kuleuven.be.\n\nThanks! </string>
    

